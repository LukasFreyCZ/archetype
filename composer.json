{
    "name": "ajthinking/php-file-manipulator",
    "type": "package",
    "description": "Programatically edit PHP and Laravel files.",
    "keywords": [
        "php",
        "laravel",
        "ast",
        "abstract syntax tree",
        "php-parser"
    ],
    "authors": [
        {
            "name": "Anders JÃ¼risoo",
            "email": "jurisoo@hotmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "nikic/php-parser": "^4.2"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Ajthinking\\PHPFileManipulator\\PHPFileManipulatorServiceProvider"
            ],
            "aliases": {
                "PHPFile": "Ajthinking\\PHPFileManipulator\\Facades\\PHPFile",
                "LaravelFile": "Ajthinking\\PHPFileManipulator\\Facades\\LaravelFile"
            }            
        }
    },
    "autoload": {
        "psr-4": {
            "Ajthinking\\PHPFileManipulator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}