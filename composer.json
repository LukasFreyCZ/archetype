{
    "name": "ajthinking/archetype",
    "type": "package",
    "description": "Programatically edit PHP and Laravel files.",
    "keywords": [
        "php",
        "laravel",
        "ast",
        "abstract syntax tree",
        "php-parser"
    ],
    "authors": [
        {
            "name": "Anders JÃ¼risoo",
            "email": "jurisoo@hotmail.com"
        }
    ],
    "license": "MIT",
    "require": {

    },
    "require-dev": {

    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Archetype\\ServiceProvider"
            ],
            "aliases": {
                "PHPFile": "Archetype\\Facades\\PHPFile",
                "LaravelFile": "Archetype\\Facades\\LaravelFile"
            }            
        }
    },
    "autoload": {
        "psr-4": {
            "Archetype\\": "src/",
            "Archetype\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}